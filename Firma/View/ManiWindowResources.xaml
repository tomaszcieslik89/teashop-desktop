    <!-- Proba nawigacji po lewej -->
    <Style x:Key="Proba2" TargetType="{x:Type Button}">
        <Setter Property="Margin" Value="3"/>
        <Setter Property="FontFamily" Value="Lato"/>
        <Setter Property="FontSize" Value="15"/>
        <Setter Property="Padding" Value="7"/>
        <Setter Property="Background" Value="#FF293746"/>
        <Setter Property="BorderBrush" Value="#FF293746"/>
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="Margin" Value="60,0,0,20"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border Background="{TemplateBinding Background}" BorderBrush="#FF293746" BorderThickness="1">
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#FF36495D"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <!--
  This style ensures that the borders in the main window are consistent.
  -->
    <Style x:Key="MainBorderStyle" TargetType="{x:Type Border}">
        <Setter Property="Background" Value="WhiteSmoke" />
        <Setter Property="BorderBrush" Value="LightGray" />
        <Setter Property="BorderThickness" Value="1" />
